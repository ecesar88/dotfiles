# ///////////////////////////////// Volume /////////////////////////////////////

# PulseAudio
bindsym $mod+m exec --no-startup-id pavucontrol

# Volume bindings
# Volume up
bindsym $mod+Ctrl+Up exec --no-startup-id pactl set-sink-volume 0 +5%

# Volume down
bindsym $mod+Ctrl+Down exec --no-startup-id pactl set-sink-volume 0 -5%

# Volume mute
bindsym $mod+Ctrl+m exec --no-startup-id pactl set-sink-mute @DEFAULT_SINK@ toggle


# Playback controls
#   Previous  MPD track
bindsym shift+F5 exec --no-startup-id mpc prev

# Pause MPD Playback
bindsym shift+F6 exec --no-startup-id mpc pause && dunstify -i ~/_my-scripts/_icons/mpd-52px.png -u normal "MPD" "Pause"

# Unpause MPD Playback
bindsym shift+F7 exec --no-startup-id mpc play && dunstify -i ~/_my-scripts/_icons/mpd-52px.png -u normal "MPD" "Play"

# Next MPD track
bindsym shift+F8 exec --no-startup-id mpc next 

# Additional playback controls
# Stop the playback
bindsym ctrl+shift+F5 exec --no-startup-id mpc stop && dunstify -i ~/_my-scripts/_icons/mpd-52px.png -u normal "MPD" "Stopped playback"

# Repeat the playlist
bindsym ctrl+shift+F6 exec --no-startup-id mpc repeat && dunstify -i ~/_my-scripts/_icons/mpd-52px.png -u normal "MPD" "Repeat mode $(~/_my-scripts/mpd/repeat.sh)"

# Repeat single
bindsym ctrl+shift+F7 exec --no-startup-id mpc single && dunstify -i ~/_my-scripts/_icons/mpd-52px.png -u normal "MPD" "Single mode $(~/_my-scripts/mpd/single.sh)"

# Random mode
bindsym ctrl+shift+F8 exec --no-startup-id mpc random && dunstify -i ~/_my-scripts/_icons/mpd-52px.png -u normal "MPD" "Random mode $(~/_my-scripts/mpd/random.sh)"

